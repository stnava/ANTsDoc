  keywords = {Adult; Brain; Female; Humans; Magnetic Resonance Imaging; Male; Middle
	Aged; Neuroanatomy; Sex Distribution},
  pmid = {12124914},
  timestamp = {2009.06.27},
  url = {http://dx.doi.org/10.1002/ajpa.10092}
}

@ARTICLE{Allen2003,
